{
  "name": "vite-serve",
  "displayName": "Vite Serve",
  "description": "An extension designed to streamline your React development workflow by starting your existing projects with Vite with no extra configuration.",
  "version": "1.0.0",
  "publisher": "phondani0",
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vite-serve.runWithVite",
        "title": "Vite Serve: Start Vite Server"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "semantic-release": "^24.0.0",
    "typescript": "^5.4.5",
    "vsce": "^2.15.0"
  },
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/phondani0/vite-serve.git"
  },
  "dependencies": {
    "@semantic-release/git": "^10.0.1"
  }
}
